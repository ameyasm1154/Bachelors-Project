{
  "file_paths": {
    "user_goals": "user_goals.py"
  },
  "run": {
    "usersim": true,
    "warmup_mem": 4,
    "num_ep_run": 200,
    "train_freq": 5,
    "max_round_num": 11,
    "success_rate_threshold": 0.3
  },
  "agent": {
    "save_weights_file_path": "dialogue_manager_dqn_model.h5",
    "load_weights_file_path": "dialogue_manager_dqn_model.h5",
    "vanilla": true,
    "learning_rate": 1e-3,
    "batch_size": 8,
    "dqn_hidden_size": 128,
    "epsilon_init": 0.0,
    "gamma": 0.9,
    "max_mem_size": 500000
  },
  "emc": {
    "slot_error_mode": 0,
    "slot_error_prob": 0.05,
    "intent_error_prob": 0.0
  }
}